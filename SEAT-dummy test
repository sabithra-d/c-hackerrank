#include<stdio.h>
int main()
{
    int n,a[100],b[100];
    int count=0,i;
    printf("Enter n:");
    scanf("%d",&n);
    printf("Enter elemnts1:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Enter elements2:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&b[i]);
    }
    for(i=0;i<n;i++)
    {
        if(a[i] == b[i+1])
        {
            count++;
        }
        
    }
printf("Count is:");
printf("%d",count);
return 0;
}

//n=2
  a= 1 2 3
  b= 4 1 2
  count is:2
